/*---------------------------
* Clear fix
* --------------------------- */
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}

.clearfix {display: inline-block;}

/* Hides from IE-mac \*/
* html .clearfix {height: 1%;}
.clearfix {display: block;}
/* End hide from IE-mac */


.row:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}

.row {display: inline-block;}

/* Hides from IE-mac \*/
* html .row {height: 1%;}
.row {display: block;}
/* End hide from IE-mac */


