	display:inline;
	float:left;
        margin-left:8px;
	}
/* Main menu */
#mainmenu{
		position:relative;
    width:100%;/*8897*/
    height:83px;
    background:url(../images/fondo_menu.png) top no-repeat;
}

#mainmenu ul {
		position:absolute;
		top:0px;
		left:0px;
    display:block;
    height:39px;
    /*margin-left: 15px;*/
		width:100%;
}
#mainmenu ul li{
    display:inline;
    float:left;
    padding: 10px 18px 0px 18px;
    /*margin-left:2px;*/
    margin-right:2px;
}
#mainmenu ul li a{
    font-family: Arial, Helvetica, sans-serif;
    font-size:18px;
    line-height: 39px;
    color: #fff;
	font-weight:bold;
    text-decoration:none;
}
#mainmenu ul li.active{
	background:#f29b18 url(../images/menu_on.png) no-repeat;
}

#mainmenu ul li:hover{
	background:#f29b18 url(../images/menu_on.png) no-repeat;
}

#mainmenu ul li ul{
	position:absolute;
	top:54px;
	left:0px;
	height:29px;
	margin-left:16px;
        display:none;
}

#mainmenu ul li.active ul{
    display: block;
}
#mainmenu ul li ul li{
	padding: 0px 12px 0px 12px;
	height:29px;
	/*background:url(../images/sub_sep.png) no-repeat top right;*/
        margin-right:2px;
        
}
#mainmenu ul li ul li a{
	font-size:14px;
	line-height:29px;
}
#mainmenu ul li ul li.active{
	background:#fff;
-webkit-border-top-left-radius: 4px;
-webkit-border-top-right-radius: 4px;
-moz-border-radius-topleft: 4px;
-moz-border-radius-topright: 4px;
border-top-left-radius: 4px;
border-top-right-radius: 4px;
	
}
#mainmenu ul li ul li.active a{
	color:#45494c;
}

#mainmenu ul li ul li:hover{
	background:#fff;
	-webkit-border-top-left-radius: 4px;
	-webkit-border-top-right-radius: 4px;
	-moz-border-radius-topleft: 4px;
	-moz-border-radius-topright: 4px;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;

	}
#mainmenu ul li ul li:hover a{
	color:#45494c;
}
        
