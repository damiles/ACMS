ul.yiiPager {
    /*display:block;*/
    clear:both;
}
ul.yiiPager li a, .yiiPager li.page a,ul.yiiPager a:link, ul.yiiPager a:visited{
    display:inline;
    float:left;
    padding:0px;
    background:#fff;
    width:24px;
    height:24px;
    font-family:Arial;
    font-size:15px;
    font-weight:bold;
