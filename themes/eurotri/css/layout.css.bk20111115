/* Top menu */
#logo{
    position:absolute; top:0px; left:0px;
    width:264px; height: 105px;
    background:url(../images/logo.png) top no-repeat;
    text-indent:-9999px;
}
#topmenu{
	display:block;
	position:absolute;
	top:0px;
	right:0px;
	height:70px;
	width:300px;
	}

#topmenu ul{
	display:inline;
	float:right;
}
