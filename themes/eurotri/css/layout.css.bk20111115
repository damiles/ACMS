    border-radius: 5px;
    margin-right:5px;
    text-align:center;
}
ul.yiiPager .selected a , .yiiPager li a:hover{
    display:inline;
    float:left;
    width:24px;
    height:24px;
    font-family:Arial;
    font-size:15px;
    font-weight:bold;
    color:#f29b18;
    line-height:23px;
    border:1px solid #f29b18;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    margin-right:5px;
    text-align:center;
}
.yiiPager li.previous a,
.yiiPager li.next a,
.yiiPager li.first a,
